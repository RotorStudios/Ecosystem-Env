{
'tool': 'maya', 
'version': '2015',
'platforms': [ 'windows', 'linux', 'darwin' ],
'requires': [ 'mtoa' ],

'environment': {
    'RS_DEPARTMENT': 'POST',
    'MAYA_version': '2015',
    'PYTHON_version': '2.7.9',
    'DEADLINE_STANDALONE': '${RS_STUDIO_envs}/deadline/Deadline7',
    'NIM_CONNECTOR': '${RS_STUDIO_envs}/NIM/nim_connector_v1-0-0-150810',
    
    'MAYA_LOCATION': { 'darwin' : '/Applications/Autodesk/maya${MAYA_version}/Maya.app/Contents',
                       'linux'  : '/usr/autodesk/maya${MAYA_version}-x64',
                       'windows': 'C:/Program Files/Autodesk/Maya${MAYA_version}', 
    },
    'PATH': { 
        'darwin': 
                '${MAYA_LOCATION}/bin:${PYTHONHOME}/Scripts',
        'linux': 
                '${MAYA_LOCATION}/bin:${PYTHONHOME}/Scripts',
        'windows': 
            '${MAYA_LOCATION}/bin;'  \
            '${PYTHONHOME}/Scripts;'  \
            '${MAYA_LOCATION}/Python/Lib/site-packages/pymel/tools/bin;'  \
            'C:/Program Files/Common Files/Autodesk Shared/;'  \
            'C:/Program Files (x86)/Autodesk/Backburner;',
    },
    'PYTHONHOME': { 'darwin': '${MAYA_LOCATION}/Python',
                     'linux': '${MAYA_LOCATION}/Python',
                   'windows': '${MAYA_LOCATION}/Python', },
    
    'DYLD_LIBRARY_PATH': { 'darwin': '${MAYA_LOCATION}/MacOS', },
     
    'RS_STUDIO_ICONS':   '${RS_DEPLOY}/Resources',
    'MAYA_PRESETS_PATH': '${RS_STUDIO_envs}/maya/presets',
    'MAYA_PLUG_IN_PATH': { 'windows': '${MAYA_MODULE_PATH}/${MAYA_version}-windows', },
    'MAYA_MODULE_PATH': '${RS_STUDIO_envs}/maya/modules',
     
    'MI_CUSTOM_SHADER_PATH': { 'windows': '${RS_STUDIO_envs}/maya/${MAYA_version}/mentalray/win64/include', },
    
    # indirection envVars
    
    'RS_LAUNCHER_MAYA': '${RS_LAUNCHER}/RS/launch/maya',
    
    'MAYA_SCRIPT_PATH': '${RS_TOOLS_MEL}/scripts',
        'RS_TOOLS_MEL_STARTUP'  : '${MAYA_SCRIPT_PATH}/startup',
        'RS_TOOLS_MEL_OVERRIDES': '${MAYA_SCRIPT_PATH}/overrides',
    'RS_TOOLS_SHELF': '${RS_TOOLS_MEL}/shelves',
    
    'RS_BASE_PY_site_packages': {
        'darwin': '${RS_STUDIO_envs}/python/py${PYTHON_version}-osx/Lib/site-packages;', 
       'windows': '${RS_STUDIO_envs}/python/py${PYTHON_version}-win64/Lib/site-packages;', 
    },
    'RS_MAYA_PY_site_packages': {
        'darwin': '${RS_STUDIO_envs}/maya/${MAYA_version}/python/py${PYTHON_version}-osx/lib/site-packages', 
       'windows': '${RS_STUDIO_envs}/maya/${MAYA_version}/python/py${PYTHON_version}-win64/lib/site-packages', 
    },
    
    'RS_IACTIVE_TOOLS': 'I:/tools',
    
    
    'PYTHONPATH': '${RS_BASE_PY_site_packages};'  \
      '${RS_MAYA_PY_site_packages}' \
      '${RS_PY_TOOLS};'  \
      '${RS_LAUNCHER};'  \
      '${RS_LAUNCHER_MAYA}/${RS_DEPARTMENT};'  \
      '${DEADLINE_STANDALONE}/python;'  \
#      '${RS_IACTIVE_TOOLS};'  \
#      '${RS_IACTIVE_TOOLS}/vred;'  \
#      '${RS_IACTIVE_TOOLS}/python;'  \
#      '${RS_IACTIVE_TOOLS}/maya;'  \
      '${NIM_CONNECTOR}',
    },


'optional': {
    'stable': {
        'STUDIO_version': 'v0002',
        'CHANNEL': 'stable',
        'RS_DEPLOY'   : '${RS_SW_BASE}/Production/Deploy',
        'RS_DEPLOY_studio': '${RS_DEPLOY}/studio/${STUDIO_version}',
        
        'RS_LAUNCHER' : '${RS_DEPLOY_studio}/launcher',
        'RS_PY_TOOLS' : '${RS_DEPLOY_studio}/tools/python;${RS_DEPLOY_studio}/python',
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel',
    
    },
    'rc': {
        'CHANNEL': 'rc',
        'RS_DEPLOY' : '${RS_SW_BASE}/DEV/Deploy',
        'RS_DEPLOY_studio': '${RS_DEPLOY}/studio/studio-${CHANNEL}',
    
        'RS_LAUNCHER' : '${RS_DEPLOY_studio}/launcher/src',
        'RS_PY_TOOLS' : '${RS_DEPLOY_studio}/tools/python/src;${RS_DEPLOY_studio}/src/python',
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel/src',
    },
    'beta': {
        'CHANNEL': 'beta',
        'RS_DEPLOY' : '${RS_SW_BASE}/DEV/Deploy',
        'RS_DEPLOY_studio': '${RS_DEPLOY}/studio/studio-${CHANNEL}',
    
        'RS_LAUNCHER' : '${RS_DEPLOY_studio}/launcher/src',
        'RS_PY_TOOLS' : '${RS_DEPLOY_studio}/tools/python/src;${RS_DEPLOY_studio}/src/python',
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel/src',
    },
    
    'alpha': {
        'CHANNEL': 'alpha',
        'RS_DEPLOY' : '${RS_SW_BASE}/DEV/Deploy',
        'RS_DEPLOY_studio': '${RS_DEPLOY}/studio/studio-${CHANNEL}',
        
        'RS_LAUNCHER' : '${RS_DEPLOY_studio}/launcher/src',
        'RS_PY_TOOLS' : '${RS_DEPLOY_studio}/tools/python/src;${RS_DEPLOY_studio}/src/python',
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel/src',
    },
    
    'assetlib': {
        'CHANNEL': 'assetlib',
        'RS_DEPLOY' : '${RS_SW_BASE}/DEV/Deploy',
        'RS_DEPLOY_studio': '${RS_DEPLOY}/studio/studio-${CHANNEL}',
        
        'RS_LAUNCHER' : '${RS_DEPLOY_studio}/launcher/src',
        'RS_PY_TOOLS' : '${RS_DEPLOY_studio}/tools/python/src;${RS_DEPLOY_studio}/src/python',
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel/src',
    },
    
    'dev': {
        'CHANNEL': 'dev',
        # RS_DEPLOY - in dev.env
        # RS_DEPLOY_studio - in dev.env
        # RS_LAUNCHER - in dev.env
        # RS_PY_TOOLS - in dev.env
        'RS_TOOLS_MEL': '${RS_DEPLOY_studio}/tools/mel/src',
        
        'RS_IACTIVE_SHELF': '${RS_IACTIVE_TOOLS}/maya/scripts/shelves',
    },
    },
}

